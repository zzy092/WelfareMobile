AA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,OAAA,GAAA,IAAA;AACA,SAFA;AAGA,OAJA,MAIA;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAFA;AAGA;AACA,KAjBA;AAkBA,IAAA,WAAA,EAAA,uBAAA;AACA;AACA,MAAA,KAAA,CAAA,eAAA;AACA,UAAA,OAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,QAAA,CAAA,IAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,OAAA;AAAA,OAAA,CAAA;;AACA,UAAA,OAAA,EAAA;AACA,YAAA,UAAA,GAAA,KAAA,aAAA,EAAA;AACA,QAAA,cAAA,CAAA,UAAA,CAAA,cAAA;AACA,QAAA,cAAA,CAAA,OAAA,CAAA,cAAA,EAAA,IAAA,CAAA,SAAA,CAAA,UAAA,CAAA,EAHA,CAIA;;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,iBADA;AAEA,UAAA,KAAA,EAAA;AACA,YAAA,UAAA,EAAA;AADA;AAFA,SAAA;AAMA,OAXA,MAWA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA;AACA,KApCA;AAqCA,IAAA,aArCA,2BAqCA;AACA,UAAA,QAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,QAAA;AACA,aAAA,QAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,OAAA;AACA,OAFA,CAAA;AAGA;AA1CA;AAxBA,CAAA","sourcesContent":["<template>\n  <div class=\"bottom-menu\">\n    <CheckButton\n      class=\"select-all\"\n      @checkBtnClick=\"checkBtnClick\"\n      v-model=\"isSelectAll\"\n    ></CheckButton>\n    <span>全选</span>\n    <span class=\"total-price\">合计: ¥{{ totalPrice }}</span>\n    <span class=\"buy-product\" @click=\"submitOrder\"\n      >去结算({{ getSumNumber }}件)\n    </span>\n  </div>\n</template>\n\n<script>\n/* eslint-disable */\nimport CheckButton from \"./CheckButton\";\n\nexport default {\n  name: \"BottomBar\",\n  components: {\n    CheckButton,\n  },\n  computed: {\n    totalPrice() {\n      const checkItems = this.getCheckItems();\n      return checkItems\n        .reduce((preValue, item) => {\n          return preValue + item.product_num * item.product_ptprice;\n        }, 0)\n        .toFixed(2);\n    },\n    isSelectAll: function () {\n      return (\n        this.$store.getters.cartList.find((item) => item.checked === false) ===\n        undefined\n      );\n    },\n    getSumNumber() {\n      return this.$store.getters.getSumCartNumber;\n    },\n  },\n  methods: {\n    checkBtnClick: function () {\n      // 1.判断是否有未选中的按钮\n      let isSelectAll = this.$store.getters.cartList.find(\n        (item) => !item.checked\n      );\n\n      // 2.有未选中的内容, 则全部选中\n      if (isSelectAll) {\n        this.$store.state.cartList.forEach((item) => {\n          item.checked = true;\n        });\n      } else {\n        this.$store.state.cartList.forEach((item) => {\n          item.checked = false;\n        });\n      }\n    },\n    submitOrder: function () {\n      //span 阻止时间冒泡\n      event.stopPropagation();\n      let isCheck = this.$store.getters.cartList.find((item) => item.checked);\n      if (isCheck) {\n        let checkItems = this.getCheckItems();\n        sessionStorage.removeItem(\"confirmItems\");\n        sessionStorage.setItem(\"confirmItems\", JSON.stringify(checkItems));\n        // sessionStorage.setItem()\n        this.$router.push({\n          path: \"/confirmOrderJd\",\n          query: {\n            submitType: 1,\n          },\n        });\n      } else {\n        console.log(\"请勾选商品\");\n      }\n    },\n    getCheckItems() {\n      const cartList = this.$store.getters.cartList;\n      return cartList.filter((item) => {\n        return item.checked;\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.bottom-menu {\n  width: 100%;\n  height: 44px;\n  background-color: #eee;\n  position: fixed;\n  bottom: 50px;\n  left: 0;\n  box-shadow: 0 -2px 3px rgba(0, 0, 0, 0.2);\n  font-size: 14px;\n  color: #888;\n  line-height: 44px;\n  padding-left: 35px;\n  box-sizing: border-box;\n}\n\n.bottom-menu .select-all {\n  position: absolute;\n  line-height: 0;\n  left: 12px;\n  top: 13px;\n}\n\n.bottom-menu .total-price {\n  margin-left: 15px;\n  font-size: 16px;\n  color: #666;\n}\n\n.bottom-menu .buy-product {\n  background-color: orangered;\n  color: #fff;\n  width: 100px;\n  height: 44px;\n  text-align: center;\n  line-height: 44px;\n  float: right;\n}\n</style>\n"],"sourceRoot":"src/views/cart/childComps"}]}